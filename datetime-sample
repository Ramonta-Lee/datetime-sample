import dayjs from "dayjs";
import moment from "moment";

// Moment.js examples
console.log(moment().format("MMMM Do YYYY, h:mm:ss a")); // e.g., October 18th 2024, 4:53:22 am
let now = moment();
console.log(now.format("MMMM YYYY")); // e.g., October 2024

// Day.js examples
console.log(dayjs().format("dddd, MMMM D, YYYY h:mm A")); // e.g., Friday, October 18, 2024 4:53 AM
